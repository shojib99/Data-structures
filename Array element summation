{
 int num[3];
 int i,sum;
  cout<<"Enter three number:"<<endl;
 {
  for( i=0;i<3;i++)
   {
     cin>>num[i];
   }
 }
{
 for(i=0;i<3;i++)
  {
    cout<<"Numbers are:"<<num[i]<<endl;
  }
}

  sum=num[0]+num[1]+num[2];
   cout<<"sum is: "<<sum;

